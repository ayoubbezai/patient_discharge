{
  "name": "zwawla-frontend",
  "version": "1.0.0",
  "description": "Zwawla Stadium Management Platform - Frontend Application",
  "author": "Zwawla Team",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zwawla/frontend.git"
  },
  "bugs": {
    "url": "https://github.com/zwawla/frontend/issues"
  },
  "homepage": "https://zwawla.com",
  "keywords": [
    "stadium",
    "booking",
    "football",
    "sports",
    "management",
    "algeria",
    "nextjs",
    "typescript"
  ],
  "scripts": {
    "dev": "next dev -p 3004",
    "build": "next build",
    "start": "next start -p 3004",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "echo \"No tests configured yet\" && exit 0",
    "test:watch": "echo \"No tests configured yet\" && exit 0",
    "test:coverage": "echo \"No tests configured yet\" && exit 0",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next out node_modules",
    "prepare": "echo \"Husky setup\"",
    "docker:build": "docker build -t zwawla-frontend .",
    "docker:run": "docker run -p 3004:3004 zwawla-frontend",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-query": "^5.28.0",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.561.0",
    "next": "16.0.10",
    "react": "19.2.1",
    "react-day-picker": "^8.9.1",
    "react-dom": "19.2.1",
    "recharts": "^3.6.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "prettier": "^3.1.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
